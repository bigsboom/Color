
page{
  height: 100%;
  font-size: 36rpx;
}
@font-face {
  font-family: 'iconfont';  /* project id 863512 */
  src: url('//at.alicdn.com/t/font_863512_3dirtx6a6ul.eot');
  src: url('//at.alicdn.com/t/font_863512_3dirtx6a6ul.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_863512_3dirtx6a6ul.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_863512_3dirtx6a6ul.woff') format('woff'),
  url('//at.alicdn.com/t/font_863512_3dirtx6a6ul.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_863512_3dirtx6a6ul.svg#iconfont') format('svg');
}
.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.container{
  height: 100%;
  width: 100%;
}
.main{
  position: relative;
  height: 100%;
  width: 100%;
}
.tips_cli{
  position: absolute;
  right: 30rpx;
  top: 30rpx;
  padding: 20rpx;
  font-size: 40rpx;
}
.tips_cli:before{
  content: "\e67f";
}
.first_guide{
  z-index: 11;
  position: absolute;
  height: 100%;
  width: 100%;
  /* overflow: hidden; */
}
.guide_box{
  position: absolute;
  height: 100%;
  width: 100%;
  z-index: 15;
  color: #ddd;
}
.guide_roll{
  margin: 110rpx auto 0;
  height: 320rpx;
  width: 320rpx;
  border-radius: 50%;
  border: 10rpx dashed #ddd;
}
.guide_top{
  position: absolute;
  width: 200rpx;
  top: 110rpx;
  left: 10rpx;
}
.guide_rectangle{
  margin: 30rpx auto 0;
  height: 400rpx;
  width: 520rpx;
  border-radius: 10%;
  border: 10rpx dashed #ddd;
}
.guide_main{
  margin: 10rpx auto 0;
  text-align: center;
}
.guide_bottom, .guide_mail {
  position: absolute;
  bottom: 60rpx;
  font-size: 30rpx;
  right: 40rpx;
}
.guide_mail {
  bottom: 20rpx;
}
.color_roll{
  margin: 130rpx auto 0;
  height: 300rpx;
  width: 300rpx;
  border-radius: 50%;
  background-color: #cd3a5c;
  /* margin-top:50rpx; */
}
.color_text{
  margin-top: 50rpx;
  height: 400rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-direction: column;
}
.text_r {
  padding-top: 15rpx;
  color: #333;
  padding-bottom: 2rpx;
  border-bottom: 1rpx solid #333;
  margin-bottom: 13rpx;
}
.picker{
  z-index: 9;
  position: absolute;
  top:0;
  height: 100%;
  width: 100%;
}
.picker_blank{
  position: absolute;
  height: 100%;
  width: 100%;
  background-color: #111;
  opacity: .5;
  z-index: 8;
}
.picker_main{
  position: absolute;
  height: 60%;
  width:100%;
  bottom: 0rpx;
  background-color: #fff;
  z-index: 9;
  display: flex;
  flex-direction: column;
}
.picker_main_title{
  display: flex;
  box-sizing: border-box;
  justify-content: space-between;
  padding:30rpx;
  width: 100%;
  /* border-bottom: 1rpx solid #ccc; */
}
.picker_main_title_no{
  color:darkgray;
}
.picker_main_title_yes{
  color:dodgerblue;
}
.picker_main_list{
  /* overflow: hidden; */
  flex: 1;
  box-sizing: border-box;
  padding:20rpx 0;
  width: 100%;
  height: 600rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.picker_main_list_one{
  margin:0 60rpx;
  padding:10rpx 0;
  display: flex;
  justify-content: center;
  align-items: center;
}
.picker_main_list_bg{
  background-color: #ccc;
}
.picker_main_list_one_bg{
  width: 60rpx;
  height: 40rpx;
  margin-right: 30rpx;
  border-radius: 10%;
}
.hidden{
  display: none;
}